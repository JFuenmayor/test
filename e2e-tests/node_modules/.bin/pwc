#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/dist/bin/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/dist/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/dist/bin/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/dist/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/@currents+playwright@0.8.1/node_modules:/Users/jorgefuenmayor/Documents/postal/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/dist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@currents+playwright@0.8.1/node_modules/@currents/playwright/dist/bin/index.js" "$@"
fi
