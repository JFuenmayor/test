{
  "name": "@postal-io/e2e-tests",
  "version": "7.0.0",
  "dependencies": {
    "@4tw/cypress-drag-drop": "^2.2.4",
    "@currents/playwright": "^0.8.0",
    "@cypress/skip-test": "^2.6.1",
    "@faker-js/faker": "^8.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@playwright/test": "1.39.0",
    "@postal-io/eslint-config": "workspace:*",
    "@postal-io/ts-config": "workspace:*",
    "@testing-library/cypress": "^9.0.0",
    "cy-verify-downloads": "^0.1.16",
    "cypress": "^12.17.4",
    "cypress-cloud": "1.9.6",
    "cypress-fill-command": "^1.0.2",
    "cypress-real-events": "^1.10.1",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.1",
    "jwt-decode": "^3.1.2",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/testing-library__cypress": "^5.0.10",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-plugin-cypress": "^2.15.1",
    "typescript": "^5.2.2"
  },
  "private": true,
  "scripts": {
    "brand:local": "URL=http://127.0.0.1:3000 pnpm playwright test test/brand --workers=7 --grep-invert @excludeFromLocalRuns",
    "brand:remote": "URL=https://test.postal.dev pnpm playwright test test/brand --workers=7",
    "brand:ui": "pnpm playwright test test/brand --ui",
    "cur:remote": "test -n \"$CI_BUILD_ID\" && pnpm cypress-cloud --config watchForFileChanges=false,baseUrl=https://test.postal.dev --parallel --record --ci-build-id $CI_BUILD_ID --browser chrome",
    "cur:run": "test -n \"$CI_BUILD_ID\" && pnpm cypress-cloud --config watchForFileChanges=false --parallel --record --ci-build-id $CI_BUILD_ID --browser chrome",
    "enagage:play:ui": "pnpm playwright test test/engage/e2e --ui",
    "engage:cy:open": "cypress open",
    "engage:cy:open:full": "CYPRESS_testUrl=http://localhost:3000 cypress open",
    "engage:cy:open:test": "CYPRESS_baseUrl=https://test.postal.dev cypress open",
    "engage:cy:run": "cypress run --headless",
    "engage:cy:run:full": "CYPRESS_testUrl=http://localhost:3000 cypress run --headless",
    "engage:play:local": "URL=http://127.0.0.1:3000 pnpm playwright test test/engage/e2e --workers=7 --grep-invert @excludeFromLocalRuns --retries=3",
    "engage:play:remote": "URL=https://test.postal.dev pnpm playwright test test/engage/e2e --workers=7 --retries=3",
    "lint": "eslint 'test/**/*.+(js|jsx|ts|tsx)'",
    "vendor:local": "URL=http://127.0.0.1:3000 pnpm playwright test test/vendor",
    "vendor:remote": "URL=https://test.postal.dev pnpm playwright test test/vendor",
    "vendor:ui": "pnpm playwright test test/vendor --ui",
    "warehouse:local": "URL=http://localhost:3000 pnpm playwright test test/warehouse --workers=7",
    "warehouse:remote": "URL=https://test.postal.dev pnpm playwright test test/warehouse --workers=7",
    "warehouse:ui": "pnpm playwright test test/warehouse --ui"
  }
}
